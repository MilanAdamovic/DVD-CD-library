<Window x:Class="DvdClubFinal.DodavanjeFilmova"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DodavanjeFilmova" Height="350" Width="350">
    <Grid>
        <Label Content="Naziv filma" HorizontalAlignment="Left" Margin="39,47,0,0" VerticalAlignment="Top"/>
        <Label Content="Zanr" HorizontalAlignment="Left" Margin="39,108,0,0" VerticalAlignment="Top"/>
        <Label Content="Trajanje" HorizontalAlignment="Left" Margin="39,166,0,0" VerticalAlignment="Top"/>
        <TextBox Name="TextBoxNFilma" HorizontalAlignment="Left" Height="23" Margin="136,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBoxZanr" HorizontalAlignment="Left" Height="23" Margin="136,108,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBoxTrajanje" HorizontalAlignment="Left" Height="23" Margin="136,166,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Name="ButtonDodajFilm" Content="Dodaj film" HorizontalAlignment="Left" Margin="56,258,0,0" VerticalAlignment="Top" Width="75" Click="ButtonDodajFilm_Click"/>
        <Button Name="ButtonOdustani" Content="Odustani" HorizontalAlignment="Left" Margin="209,258,0,0" VerticalAlignment="Top" Width="75" Click="ButtonOdustani_Click"/>
    </Grid>
</Window>
